{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/master/docs/sources/developers/plugins/plugin.schema.json",
  "type": "datasource",
  "name": "CnosDB",
  "id": "cnos-grafana-datasource",
  "category": "tsdb",

  "alerting": true,
  "annotations": true,
  "logs": true,
  "metrics": true,
  "streaming": false,
  "tracing": false,

  "backend": true,
  "executable": "gpx_cnosdb",

  "queryOptions": {
    "minInterval": true
  },

  "info": {
    "description": "A grafana data source plugin for CnosDB.",
    "author": {
      "name": "CnosDB",
      "url": "https://github.com/cnosdb/cnosdb"
    },
    "keywords": [
      "cnosdb",
      "tsdb",
      "timeseries"
    ],
    "logos": {
      "small": "img/logo.png",
      "large": "img/logo.png"
    },
    "links": [
      {
        "name": "Website",
        "url": "https://github.com/cnosdb/cnosdb"
      },
      {
        "name": "License",
        "url": "https://github.com/grafana/grafana-starter-datasource/blob/master/LICENSE"
      }
    ],
    "screenshots": [
      {
        "name": "Config Editor",
        "path": "img/config_editor.png"
      },
      {
        "name": "Query Editor",
        "path": "img/query_editor.png"
      },
      {
        "name": "Dashboard",
        "path": "img/dashboard.png"
      }
    ],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "dependencies": {
    "grafanaDependency": ">=8.0.0",
    "plugins": []
  }
}
