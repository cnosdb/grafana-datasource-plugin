    "@types/lodash": "latest",
    "@types/js-base64": "latest",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "12.1.4",
    "@types/debounce-promise": "3.1.2"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "debounce-promise": "3.1.2"
